#Listen Turtle


#Import turt and name
import turtle
t = turtle.Turtle()

#make it perrrrrrty
t.color("violetred2", "hotpink")
t.shape("turtle")

for i in range(5):
  x = input("Put number here")
  t.left(x)
  t.forward(100)
